                      td.column.column-3(style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; font-weight: 400; text-align: left; vertical-align: top; border-top: 0px; border-right: 0px; border-bottom: 0px; border-left: 0px;' width='16.666666666666668%')
                        .spacer_block(style='height:10px;line-height:5px;font-size:1px;') &hairsp;
