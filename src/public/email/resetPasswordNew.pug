doctype html
head
  title
  meta(content='text/html; charset=utf-8' http-equiv='Content-Type')
  meta(content='width=device-width, initial-scale=1.0' name='viewport')
  link(href='https://fonts.googleapis.com/css?family=Nunito' rel='stylesheet' type='text/css')
  style.
    * {
    box-sizing: border-box;
    }
    body {
    margin: 0;
    padding: 0;
    }
    a[x-apple-data-detectors] {
    color: inherit !important;
    text-decoration: inherit !important;
    }
    #MessageViewBody a {
    color: inherit;
    text-decoration: none;
    }
    p {
    line-height: inherit
    }
    @media (max-width:715px) {
    .icons-inner {
    text-align: center;
    }
    .icons-inner td {
    margin: 0 auto;
    }
    .fullMobileWidth,
    .row-content {
    width: 100% !important;
    }
    .image_block img.big {
    width: auto !important;
    }
    .column .border {
    display: none;
    }
    table {
    table-layout: fixed !important;
    }
    .stack .column {
    width: 100%;
    display: block;
    }
    }
table.nl-container(border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; background-color: #d8eeec;' width='100%')
  tbody
    tr
      td
        table.row.row-1(align='center' border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; background-color: #d8eeec;' width='100%')
          tbody
            tr
              td
                table.row-content.stack(align='center' border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; background-color: #ffffff; color: #000000; width: 695px;' width='695')
                  tbody
                    tr
                      td.column.column-1(style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; font-weight: 400; text-align: left; vertical-align: top; border-top: 0px; border-right: 0px; border-bottom: 0px; border-left: 0px;' width='33.333333333333336%')
                        .spacer_block(style='height:10px;line-height:5px;font-size:1px;') &hairsp;
                      td.column.column-2(style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; font-weight: 400; text-align: left; vertical-align: top; border-top: 0px; border-right: 0px; border-bottom: 0px; border-left: 0px;' width='33.333333333333336%')
                        table.image_block(border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt;' width='100%')
                          tr
                            td(style='width:100%;padding-right:0px;padding-left:0px;padding-top:5px;padding-bottom:5px;')
                              div(align='center' style='line-height:10px')
                                a(href='https://mobiusorg.netlify.app' style='outline:none' tabindex='-1' target='_blank')
                                  img(alt='Company Logo' src='images/Group_66_logo.png' style='display: block; height: auto; border: 0; width: 104px; max-width: 100%;' title='Company Logo' width='104')
                      td.column.column-3(style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; font-weight: 400; text-align: left; vertical-align: top; border-top: 0px; border-right: 0px; border-bottom: 0px; border-left: 0px;' width='33.333333333333336%')
                        .spacer_block(style='height:10px;line-height:5px;font-size:1px;') &hairsp;
        table.row.row-2(align='center' border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt;' width='100%')
          tbody
            tr
              td
                table.row-content.stack(align='center' border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; background-color: #ffffff; color: #000000; width: 695px;' width='695')
                  tbody
                    tr
                      td.column.column-1(style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; font-weight: 400; text-align: left; vertical-align: top; padding-top: 5px; padding-bottom: 5px; border-top: 0px; border-right: 0px; border-bottom: 0px; border-left: 0px;' width='100%')
                        table.image_block(border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt;' width='100%')
                          tr
                            td(style='width:100%;padding-right:0px;padding-left:0px;padding-bottom:5px;')
                              div(align='center' style='line-height:10px')
                                img.fullMobileWidth.big(alt='bear looking at password' src='images/Forgot_Password.gif' style='display: block; height: auto; border: 0; width: 348px; max-width: 100%;' title='bear looking at password' width='348')
        table.row.row-3(align='center' border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt;' width='100%')
          tbody
            tr
              td
                table.row-content.stack(align='center' border='0' cellpadding='0' cellspacing='0' role='presentation' style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; background-color: #ffffff; color: #000000; width: 695px;' width='695')
                  tbody
                    tr
                      td.column.column-1(style='mso-table-lspace: 0pt; mso-table-rspace: 0pt; font-weight: 400; text-align: left; vertical-align: top; border-top: 0px; border-right: 0px; border-bottom: 0px; border-left: 0px;' width='16.666666666666668%')
